:60,"processing_pictures":-2,"guides_count":0,"main_image_path":"https://s3.amazonaws.com/openfarm-project/production/media/pictures/attachments/59d3d95f35bc1f0004000259.jpg?1507055959","taxon":"Species","tags_array":[],"growing_degree_days":null,"svg_icon":null},"links":{"self":{"api":"/api/v1/crops/54afdcfa3166630002080400","website":"/crops/54afdcfa3166630002080400"}},"relationships":{"companions":{"links":{"related":"/api/v1/crops/54afdcfa3166630002080400/companions"}},"pictures":{"links":{"related":"/api/v1/crops/54afdcfa3166630002080400/pictures"}}}},{"type":"crops","id":"54afdd8b3166630002f40500","attributes":{"name":"artemisiae vulgaris herba","slug":"artemisiae-vulgaris-herba","binomial_name":"artemisia vulgaris","common_names":["artemisiae vulgaris herba"," tarragon"," estragon"," dragonwort"," felonherb"," sailor's-tobacco"," mugwort"," estragao"," artemisiae vulgaris radix"," beifuss"],"description":null,"sun_requirements":null,"sowing_method":null,"spread":null,"row_spacing":null,"height":null,"processing_pictures":0,"guides_count":0,"main_image_path":"/assets/baren_field_square-4a827e5f09156962937eb100e4484f87e1e788f28a7c9daefe2a9297711a562a.jpg","taxon":null,"tags_array":[],"growing_degree_days":null,"svg_icon":null},"links":{"self":{"api":"/api/v1/crops/54afdd8b3166630002f40500","website":"/crops/54afdd8b3166630002f40500"}},"relationships":{"companions":{"links":{"related":"/api/v1/crops/54afdd8b3166630002f40500/companions"}},"pictures":{"links":{"related":"/api/v1/crops/54afdd8b3166630002f40500/pictures"}}}},{"type":"crops","id":"5a1c96322ae5000004000023","attributes":{"name":"Rue","slug":"rue","binomial_name":"Ruta graveolens","common_names":["common rue","rue","herb-of-grace"],"description":"Rue, or Common Rue, is a woody-based, shrubby perennial with aromatic, musty, blue-green leaves. Rue is cultivated as an ornamental plant (because of it's leaves) and an herb. It grows in a mounded shape and tolerates heat and drought well once it is established. Rue was used for culinary and medicinal purposes in Ancient Rome and during the Middle Ages, but has fallen out of use because it has a bitter taste and large doses can be toxic. Handling the plants or using extracts can cause skin blistering, so gloves are recommended. Avoid planting near cabbage, basil, or sage.","sun_requirements":"Full Sun","sowing_method":"Direct seed or cuttings","spread":90,"row_spacing":null,"height":90,"processing_pictures":0,"guides_count":0,"main_image_path":"https://s3.amazonaws.com/openfarm-project/production/media/pictures/attachments/5a1c96376dd6c00004000002.jpg?1511822900","taxon":"Species","tags_array":[],"growing_degree_days":null,"svg_icon":null},"links":{"self":{"api":"/api/v1/crops/5a1c96322ae5000004000023","website":"/crops/5a1c96322ae5000004000023"}},"relationships":{"companions":{"links":{"related":"/api/v1/crops/5a1c96322ae5000004000023/companions"}},"pictures":{"links":{"related":"/api/v1/crops/5a1c96322ae5000004000023/pictures"}}}}]}